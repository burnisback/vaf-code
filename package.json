{
  "name": "vaf-code",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:emulators": "concurrently \"npm run dev\" \"npm run emulators\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "emulators": "firebase emulators:start",
    "emulators:export": "firebase emulators:export ./emulator-data",
    "emulators:import": "firebase emulators:start --import=./emulator-data",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@genkit-ai/googleai": "^1.27.0",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@webcontainer/api": "^1.1.9",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/xterm": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^9.23.0",
    "framer-motion": "^12.24.0",
    "genkit": "^1.27.0",
    "handlebars": "^4.7.8",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^4.2.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "null-loader": "^4.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
